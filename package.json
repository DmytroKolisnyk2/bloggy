{
  "name": "bloggyApp",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0 <17.0.0"
  },
  "scripts": {
    "start": "next",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "serve": "next start",
    "prettier:check": "prettier --check \"*/**/*.{ts,tsx}\" --loglevel=warn",
    "prettier:fix": "prettier --write \"*/**/*.{ts,tsx}\" --loglevel=warn",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "hooks:pre-commit": "npm run prettier:fix && npm run lint:fix"
  },
  "sideEffects": false,
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@hookform/resolvers": "2.9.7",
    "@reduxjs/toolkit": "1.8.5",
    "emotion-normalize": "11.0.1",
    "joi": "17.6.0",
    "next": "12.3.0",
    "next-i18next": "12.0.1",
    "next-redux-wrapper": "7.0.5",
    "nextjs-progressbar": "^0.0.14",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.34.2",
    "react-redux": "8.0.2",
    "react-transition-group": "4.4.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.3.0",
    "@types/node": "18.7.16",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.1",
    "next-bundle-analyzer": "^0.6.4",
    "prettier": "2.7.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run hooks:pre-commit"
    }
  }
}
